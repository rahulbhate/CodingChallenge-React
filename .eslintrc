{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "localStorage": true,
    "window": true,
    "document": true,
    "FileReader": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "overrides": [
    {
      "files": ["setupTests.js"],
      "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "quotes":[
          "error",
          "double",
          "single"
        ]
      }
    }
  ]
}